                                           <http://purl.org/vocab/aiiso/schema#School>
                                         )
                           ] .


###  http://ies.linkeddata.ec/syllabusOntology/vocabulary#is_taught_by
:is_taught_by rdf:type owl:ObjectProperty ;
              owl:inverseOf <http://purl.org/vocab/aiiso/schema#teaches> ;
              rdfs:domain <http://purl.org/vocab/aiiso/schema#KnowledgeGrouping> ;
              rdfs:range [ rdf:type owl:Class ;
                           owl:unionOf ( <http://xmlns.com/foaf/0.1/Organization>
                                         <http://xmlns.com/foaf/0.1/Person>
                                       )
                         ] .


###  http://purl.org/vocab/aiiso/schema#knowledgeGrouping
<http://purl.org/vocab/aiiso/schema#knowledgeGrouping> rdf:type owl:ObjectProperty ;
                                                       rdfs:domain [ rdf:type owl:Class ;
                                                                     owl:unionOf ( <http://purl.org/vocab/aiiso/schema#KnowledgeGrouping>
                                                                                   <http://xmlns.com/foaf/0.1/Organization>
                                                                                 )
                                                                   ] ;
                                                       rdfs:range <http://purl.org/vocab/aiiso/schema#KnowledgeGrouping> ;
                                                       <http://purl.org/dc/terms/issued> "2008-05-14"^^xsd:string ;
                                                       rdfs:comment "A KnowledgeGrouping may be contained by another KnowledgeGrouping or an organizationalUnit using knowledgeGrouping."^^xsd:string ;
                                                       rdfs:isDefinedBy <http://purl.org/vocab/aiiso/schema#> ;
                                                       rdfs:label "knowledgeGrouping"@en ;
                                                       <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "Deprecated"^^xsd:string ;
                                                       <http://www.w3.org/2004/02/skos/core#historyNote> ""^^xsd:string .


###  http://purl.org/vocab/aiiso/schema#organization
<http://purl.org/vocab/aiiso/schema#organization> rdf:type owl:ObjectProperty ;
                                                  rdfs:domain <http://xmlns.com/foaf/0.1/Organization> ;
                                                  rdfs:range <http://xmlns.com/foaf/0.1/Organization> ;
                                                  <http://purl.org/dc/terms/issued> "2008-09-25"^^xsd:string ;
                                                  rdfs:comment "A foaf Organization may contain other subsidiary Organizations using organization."^^xsd:string ;
                                                  rdfs:isDefinedBy <http://purl.org/vocab/aiiso/schema#> ;
                                                  rdfs:label "organization"@en ;
                                                  <http://www.w3.org/2004/02/skos/core#historyNote> ""^^xsd:string .


###  http://purl.org/vocab/aiiso/schema#organizationalUnit
<http://purl.org/vocab/aiiso/schema#organizationalUnit> rdf:type owl:ObjectProperty ;
                                                        rdfs:domain <http://xmlns.com/foaf/0.1/Organization> ;
                                                        rdfs:range <http://xmlns.com/foaf/0.1/Organization> .


###  http://purl.org/vocab/aiiso/schema#part_of
<http://purl.org/vocab/aiiso/schema#part_of> rdf:type owl:ObjectProperty ;
                                             rdfs:domain <http://xmlns.com/foaf/0.1/Organization> ,
                                                         [ rdf:type owl:Class ;
                                                           owl:unionOf ( <http://purl.org/spar/doco/Section>
                                                                         <http://purl.org/vocab/aiiso/schema#Faculty>
                                                                         <http://purl.org/vocab/aiiso/schema#School>
                                                                       )
                                                         ] ;
                                             rdfs:range <http://xmlns.com/foaf/0.1/Organization> ,
                                                        [ rdf:type owl:Class ;
                                                          owl:unionOf ( <http://purl.org/spar/doco/Chapter>
                                                                        <http://purl.org/vocab/aiiso/schema#College>
                                                                        <http://purl.org/vocab/aiiso/schema#Faculty>
                                                                      )
                                                        ] ;
                                             <http://purl.org/dc/terms/issued> "2008-09-25"^^xsd:string ;
                                             rdfs:comment "Indica que una Facultad es parte de cierta Universidad y que una Carrera es parte de cierta Facultad."@es ,
                                                          "A foaf Organization may refer to its parent Organizations using part_of."^^xsd:string ;
                                             rdfs:isDefinedBy <http://purl.org/vocab/aiiso/schema#> ;
                                             rdfs:label "part of"@en ,
                                                        "parte_de"@es ;
                                             <http://www.w3.org/2004/02/skos/core#historyNote> ""^^xsd:string .


###  http://purl.org/vocab/aiiso/schema#responsibilityOf
<http://purl.org/vocab/aiiso/schema#responsibilityOf> rdf:type owl:ObjectProperty ;
                                                      owl:inverseOf <http://purl.org/vocab/aiiso/schema#responsibleFor> ;
                                                      rdfs:domain [ rdf:type owl:Class ;
                                                                    owl:unionOf ( <http://purl.org/vocab/aiiso/schema#KnowledgeGrouping>
                                                                                  <http://xmlns.com/foaf/0.1/Organization>
                                                                                )
                                                                  ] ;
                                                      rdfs:range [ rdf:type owl:Class ;
                                                                   owl:unionOf ( :Coordinador
                                                                                 :Profesor
                                                                               )
                                                                 ] ;
                                                      <http://purl.org/dc/terms/issued> "2008-05-20"^^xsd:string ;
                                                      rdfs:comment "Indica una asignatura que es responsabilidad de una persona."@es ;
                                                      rdfs:isDefinedBy <http://purl.org/vocab/aiiso/schema#> ;
                                                      rdfs:label "responsabilidad_de"@es ,
                                                                 "responsibility of"@en .


###  http://purl.org/vocab/aiiso/schema#responsibleFor
<http://purl.org/vocab/aiiso/schema#responsibleFor> rdf:type owl:ObjectProperty ;
                                                    rdfs:domain [ rdf:type owl:Class ;
                                                                  owl:unionOf ( :Coordinador
                                                                                :Profesor
                                                                              )
                                                                ] ;
                                                    rdfs:range [ rdf:type owl:Class ;
                                                                 owl:unionOf ( <http://purl.org/vocab/aiiso/schema#KnowledgeGrouping>
                                                                               <http://xmlns.com/foaf/0.1/Organization>
                                                                             )
                                                               ] ;
                                                    <http://purl.org/dc/terms/issued> "2008-09-25"^^xsd:string ;
                                                    rdfs:comment "Indica la persona que tiene como responsabilidad enseñar una asignatura."@es ;
                                                    rdfs:isDefinedBy <http://purl.org/vocab/aiiso/schema#> ;
                                                    rdfs:label "responsabilidad_para"@es ,
                                                               "responsible for"@en ,
                                                               "responsible_for"@en ;
                                                    <http://www.w3.org/2004/02/skos/core#historyNote> ""^^xsd:string .


###  http://purl.org/vocab/aiiso/schema#teaches
<http://purl.org/vocab/aiiso/schema#teaches> rdf:type owl:ObjectProperty ;
                                             rdfs:domain [ rdf:type owl:Class ;
                                                           owl:unionOf ( :Profesor
                                                                         <http://xmlns.com/foaf/0.1/Organization>
                                                                       )
                                                         ] ;
                                             rdfs:range <http://purl.org/vocab/aiiso/schema#KnowledgeGrouping> ;
                                             <http://purl.org/dc/terms/issued> "2008-09-25"^^xsd:string ;
                                             rdfs:comment "An organization may specify the Knowledge Groupings that it teaches using this property."^^xsd:string ;
                                             rdfs:isDefinedBy <http://purl.org/vocab/aiiso/schema#> ;
                                             rdfs:label "enseña"@es ,
                                                        "teaches"@en .


