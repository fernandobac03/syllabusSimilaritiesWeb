#################################################################
#    Object Properties
#################################################################

###  http://ies.linkeddata.ec/syllabusOntology/vocabulary#conformada
:conformada rdf:type owl:ObjectProperty ;
            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( <http://purl.org/vocab/aiiso/schema#College>
                                        <http://purl.org/vocab/aiiso/schema#Faculty>
                                      )
                        ] ;
            rdfs:range [ rdf:type owl:Class ;
                         owl:unionOf ( <http://purl.org/vocab/aiiso/schema#Faculty>
                                       <http://purl.org/vocab/aiiso/schema#School>
                                     )
                       ] ;
            rdfs:comment "Indica que una Universidad está conformada por cierta Facultad y que una Facultad está conformada por cierta Carrera."@es ;
            rdfs:isDefinedBy <http://linkeddata.ec/vsuc> ;
            rdfs:label "conformada_por"@es ,
                       "formed_by"@en .


###  http://ies.linkeddata.ec/syllabusOntology/vocabulary#estrategia
:estrategia rdf:type owl:ObjectProperty ;
            rdfs:domain <http://purl.org/spar/doco/Chapter> ;
            rdfs:range :Learning_Strategy ;
            rdfs:comment "Enlaza una estrategia de aprendizaje con un capítulo del contenido académico."@es ;
            rdfs:isDefinedBy <http://linkeddata.ec/vsuc> ;
            rdfs:label "estrategia_de_aprendizaje"@es ,
                       "learning_strategy"@en .


###  http://ies.linkeddata.ec/syllabusOntology/vocabulary#has_academic_content
:has_academic_content rdf:type owl:ObjectProperty ;
                      owl:inverseOf :is_academic_content_of ;
                      rdfs:domain <http://purl.org/vocab/aiiso/schema#KnowledgeGrouping> ;
                      rdfs:range :Academic_Content .


###  http://ies.linkeddata.ec/syllabusOntology/vocabulary#has_academic_unit
:has_academic_unit rdf:type owl:ObjectProperty ;
                   owl:inverseOf :is_academic_unit_of ;
                   rdfs:domain <http://purl.org/vocab/aiiso/schema#Institution> ;
                   rdfs:range [ rdf:type owl:Class ;
                                owl:unionOf ( <http://purl.org/vocab/aiiso/schema#Department>
                                              <http://purl.org/vocab/aiiso/schema#Faculty>
                                              <http://purl.org/vocab/aiiso/schema#School>
                                            )
                              ] .


###  http://ies.linkeddata.ec/syllabusOntology/vocabulary#has_bibliography
:has_bibliography rdf:type owl:ObjectProperty ;
                  owl:inverseOf :is_bibliography_of ;
                  rdfs:domain <http://purl.org/vocab/aiiso/schema#KnowledgeGrouping> ;
                  rdfs:range <http://purl.org/ontology/bibo/Document> .


###  http://ies.linkeddata.ec/syllabusOntology/vocabulary#has_chapter
:has_chapter rdf:type owl:ObjectProperty ;
             owl:inverseOf :is_chapter_of ;
             rdfs:domain :Academic_Content ;
             rdfs:range <http://purl.org/spar/doco/Chapter> .


###  http://ies.linkeddata.ec/syllabusOntology/vocabulary#has_evaluation
:has_evaluation rdf:type owl:ObjectProperty ;
                rdfs:domain :Learning_Achievement ;
                rdfs:range :Evaluation_Mode .


###  http://ies.linkeddata.ec/syllabusOntology/vocabulary#has_hei
:has_hei rdf:type owl:ObjectProperty .


###  http://ies.linkeddata.ec/syllabusOntology/vocabulary#has_indicator
:has_indicator rdf:type owl:ObjectProperty ;
               rdfs:domain :Learning_Achievement ;
               rdfs:range :Indicator .


###  http://ies.linkeddata.ec/syllabusOntology/vocabulary#has_item
:has_item rdf:type owl:ObjectProperty ;
          owl:inverseOf :is_item_of ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( :Objective
                                      :Results
                                    )
                      ] ;
          rdfs:range [ rdf:type owl:Class ;
                       owl:unionOf ( :Objective
                                     :Results
                                   )
                     ] .


###  http://ies.linkeddata.ec/syllabusOntology/vocabulary#has_objective
:has_objective rdf:type owl:ObjectProperty ;
               owl:inverseOf :is_objective_of ;
               rdfs:domain <http://purl.org/vocab/aiiso/schema#KnowledgeGrouping> ;
               rdfs:range :Objective .


###  http://ies.linkeddata.ec/syllabusOntology/vocabulary#has_result
:has_result rdf:type owl:ObjectProperty ;
            owl:inverseOf :is_result_of ;
            rdfs:domain <http://purl.org/vocab/aiiso/schema#KnowledgeGrouping> ;
            rdfs:range :Results .


